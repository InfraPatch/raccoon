  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/formidable": "^2.0.6",
    "@types/howler": "^2.2.7",
    "@types/is-url": "^1.2.30",
    "@types/lodash.omit": "^4.5.7",
