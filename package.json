    "react-dom": "18.2.0",
    "react-feather": "^2.0.10",
    "react-i18next": "^13.0.0",
    "react-signature-canvas": "^1.0.6",
    "remark-gfm": "^3.0.1",
    "sanitize-filename": "^1.6.3",
    "sass": "^1.63.4",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.6.2",
