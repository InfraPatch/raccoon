{
  "name": "raccoon",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ws": "ts-node -r tsconfig-paths/register -P tsconfig.ws.json chat/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:ws\"",
    "build": "next build",
    "build:ws": "tsc -p tsconfig.ws.json && tsc-alias -p tsconfig.ws.json",
    "start": "next start",
    "start:ws": "node dist/ws/chat/index.js",
    "lint": "eslint . --ext .ts --ext .tsx",
    "typeorm": "node ./node_modules/typeorm/cli.js --config ormconfig.cli.js",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:rollback": "npm run typeorm -- migration:revert",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migrate": "npm run migration:run",
    "update": "npx npm-check-updates -u --reject typeorm --reject camelcase"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.354.0",
    "@google-cloud/storage": "^6.11.0",
    "@sindresorhus/slugify": "^2.2.1",
