    "bcryptjs": "^2.4.3",
    "camelcase": "^6.2.0",
    "clsx": "^1.2.1",
    "concurrently": "^8.2.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "firebase-admin": "^11.9.0",
    "formidable": "^3.4.0",
    "formik": "^2.4.2",
    "fs-extra": "^11.1.1",
    "googleapis": "^118.0.0",
    "howler": "^2.2.3",
    "i18next": "^23.2.1",
    "iron-session": "^6.3.1",
    "is-url": "^1.2.4",
    "isomorphic-dompurify": "^1.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "lodash.pull": "^4.1.0",
    "lodash.remove": "^4.7.0",
    "mime-types": "^2.1.35",
    "mysql2": "^3.4.0",
    "next": "^13.4.6",
    "next-bar": "^1.0.1",
    "next-i18next": "^14.0.0",
    "next-mdx-remote": "^4.4.1",
    "nodemailer": "^6.9.3",
